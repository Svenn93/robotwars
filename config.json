{
	"stylesheets": {
		"src":{
			"folder": "_scss",
			"file":"**/*.scss"
		},
		"dest": {
			"folder":"public/css",
			"file":"screen.css"
		}
	},
	"fonts": {
		"dest": {
			"folder": "./public/fonts"
		}
	},
	"scripts": {
		"src":{
			"folder": "_js",
			"file":"**/*.js",
			"entry_file":"app.js"
		},
		"dest": {
			"folder":"public/js",
			"file":"app.js"
		}
	},
	"vendor": {
		"src":{
			"folder": "./bower_components",
			"file":"**"
		},
		"dest": {
			"file": "vendor.js"
		}
	}
}
